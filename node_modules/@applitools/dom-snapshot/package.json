{
  "name": "@applitools/dom-snapshot",
  "version": "4.15.8",
  "homepage": "https://applitools.com",
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./index.js",
    "./dist/processPagePollCjs": {
      "types": "./dist/processPagePollCjs.d.ts",
      "default": "./dist/processPagePollCjs.js"
    },
    "./dist/pollResultCjs": {
      "types": "./dist/pollResultCjs.d.ts",
      "default": "./dist/pollResultCjs.js"
    }
  },
  "files": [
    "dist",
    "src/getScript.js",
    "index.js"
  ],
  "scripts": {
    "lint": "run --top-level eslint '**/*.{js,ts}'",
    "build": "rollup -c src/build/rollup.config.js",
    "build:pretty": "APPLITOOLS_BUILD_MODE=pretty rollup -c src/build/rollup.config.js",
    "build:watch": "yarn build --watch",
    "test": "run test:mocha && run test:testcafe",
    "test:mocha": "run --top-level mocha './test/**/*.test.{js,ts}'",
    "test:testcafe": "testcafe chrome:headless test/testcafe/*.testcafe.js",
    "surge": "surge test/fixtures applitools-dom-snapshot.surge.sh"
  },
  "dependencies": {
    "@applitools/dom-shared": "1.1.1",
    "@applitools/functional-commons": "1.6.0",
    "css-tree": "^3.1.0",
    "pako": "1.0.11",
    "throat": "6.0.2"
  },
  "devDependencies": {
    "@applitools/monitoring-commons": "^1.0.19",
    "@applitools/snippets": "2.7.0",
    "@applitools/spec-driver-puppeteer": "1.7.1",
    "@applitools/spec-driver-webdriver": "1.5.6",
    "@applitools/test-server": "^1.4.0",
    "@applitools/test-utils": "^1.5.17",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.23.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/chai": "^5",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.3.0",
    "@types/pixelmatch": "^5.2.4",
    "abort-controller": "^3.0.0",
    "chai": "^4.2.0",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "cssom": "^0.4.4",
    "express": "^4.21.2",
    "jsdom": "^22.1.0",
    "node-fetch": "^2.6.0",
    "pixelmatch": "^5.3.0",
    "puppeteer": "^24.11.1",
    "rollup": "^4.1.4",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-ignore": "^1.0.10",
    "rollup-plugin-polyfill": "^4.2.0",
    "selenium-webdriver": "^4.15.0",
    "testcafe": "^1.18.1",
    "typescript": "^5.9.2",
    "whatwg-fetch": "^3.0.0"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "lavamoat": {
    "allowScripts": {
      "core-js": false,
      "jsdom>ws>bufferutil": false,
      "jsdom>ws>utf-8-validate": false,
      "puppeteer": true
    }
  }
}